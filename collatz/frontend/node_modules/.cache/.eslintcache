ome/samuel/repo/showcase/collatz/frontend/src/view/index.js",["87"],[],"import Component from './component'\nimport asset from './asset'\nimport style from './style'\n\nimport {\n\tHeader,\n\tFooter\n} from './frame'\n\nimport {Content} from './content'\n\nlet View = props => {\n\tlet {state, event, onUpdate} = props\n\n\tlet handleUpdate = (data, source) => {\n\t\tonUpdate && onUpdate(data, source)\n\t}\n\n\treturn <div style={style}>\n\t\t\t<Header\n\t\t\t\tlogo={asset.Logo}\n\t\t\t\tstate={state}\n\t\t\t\tstyle={style.header} />\n\n\t\t\t<Content\n\t\t\t\tComponent={Component}\n\t\t\t\tstate={state}\n\t\t\t\tasset={asset}\n\t\t\t\tstyle={style.content}\n\t\t\t\tonUpdate={handleUpdate} \n\t\t\t\tevent={event}/>\n\t\t</div>\n}\n\nexport default View\n","/home/samuel/repo/showcase/collatz/frontend/src/model/rest.js",[],[],"/home/samuel/repo/showcase/collatz/frontend/src/view/style.js",[],[],"/home/samuel/repo/showcase/collatz/frontend/src/model/state.js",[],[],"/home/samuel/repo/showcase/collatz/frontend/src/view/component/index.js",["88","89","90","91","92","93"],[],"\nimport {Tab as Tab, TabContainer as TabContainer} from './tab'\nimport {Inspector as Inspector} from './inspector'\nimport {Lines as Lines, Bars as Bars} from './dataview'\n\nexport default {\n\tTabContainer,\n\tTab,\n\tInspector,\n\tLines,\n\tBars\n}\n","/home/samuel/repo/showcase/collatz/frontend/src/view/content/index.js",["94","95","96","97"],[],"import {useState} from 'react'\n\nconst startNumber = 63728127n\n\nlet nextColas = n => 3n*(n % 2n ?  n + 1n : n >> 1n)\nlet nextColaz = n => n % 2n ? 3n*n + 1n : n >> 1n\n\nlet bigmax = (a,b) => a >= b ? a : b\n\nlet biglog  = bigint => {\n  if (bigint < 0) return NaN\n  const s = bigint.toString(10)\n\n  return s.length + Math.log10(\"0.\" + s.substring(0, 15))\n}\n\nlet initialize = n => {\n\tlet data = {\n\t\tcolaz : [n],\n\t\tcolas : [3n*n],\n\t\tmax : -1n\n\t}\n\n\tfor (let count=0; count < 101; count++ ){\n\t\tlet clz = nextColaz(data.colaz[0])\n\t\tlet cls = nextColas(clz)\n\n\t\tdata.colaz.unshift(clz)\n\t\tdata.colas.unshift(cls)\n\t\tdata.max = bigmax(bigmax(data.max, clz), cls)\n\t}\n\n\treturn data\n\n}\n\nconst barStyle = {\n\twidth: '900px',\n\tposition: 'absolute'\n}\n\nconst Content = props => {\n\tlet {Component,selection, state,asset, style, onUpdate} = props\n\n\tlet data = initialize(startNumber)\n\n\treturn  <div style={style}>\n\t\t\t<div style={{width: '80%'}}>\n\t\t\t\t<p>Here I present a proof of the Collatz conjecture, which is believed to be true</p>\n\n\t\t\t\t<p>\n\t\t\t\t\tConsider the sequence generated by Colas <br/>\n\t\t\t\t\tObserve that I assume natural numbers.\n\t\t\t\t</p>\n\n\t\t\t\t<img style={{height: '50px'}} src={asset.Colas} alt='Colas sequence' />\n\n\t\t\t\t<p>and the sequence generated by Collatz</p>\n\n\n\t\t\t\t<img style={{height: '50px'}} src={asset.Colaz} alt='Collatz sequence' />\n\n\t\t\t\t<p>Now assume that z(x) tends to infinity.</p>\n\t\t\t\t<p>It is easy to prove, at least, for any natural number, the following inequality, which I leave to you as exercise ...</p>\n\n\t\t\t\t<img style={{height: '30px'}} src={asset.Inequality} alt='Colas inequality' />\n\n\t\t\t\t<p>\n\t\t\t\t\tBut this inequality is a contradiction, also establishing an upper bound for z(x)<br/>\n\n\t\t\t\t\tSince s(x) converges to 1 (easy proof using induction), 3*s(x) will converge to 3. This leads to the conclusion that z(x),\n\t\t\t\t\tthe Collatz sequence, converges to 1, which is already known for x less than 3.\n\t\t\t\t</p>\n\t\t\t\t<p>\n\t\t\t\t\tOne could argue that z(x), after some large index i, stays on a constant K larger than 1. <br/>\n\t\t\t\t\tIn this case we can substitute the inequality by\n\t\t\t\t</p>\n\n\t\t\t\t<img style={{height: '30px'}}  src={asset.Kinequality} alt='Kolas inequality' />\n\n\t\t\t\t<p>Eventually, 3*s( K + i ) will converge to 3 as i goes to infinity.</p>\n\n\n\t\t\t\t<p>This is true in general, if 3 is substituted by a prime number greater than 2</p>\n\n\t\t\t\t<p>The chart is giving a hint of what is going on for 63728127 as the initial number</p>\n\t\t\t\t<p>The red bars show z(x) ) and the white bars show  3*s(z(x)), using a logarithmic scale</p>\n\n\t\t\t\t<div>\n\t\t\t\t\t<div style={barStyle}>\n\t\t\t\t\t\t<Component.Bars\n\t\t\t\t\t\t\tdata={data.colas.map(v =>biglog(v))}\n\t\t\t\t\t\t\twidth='800'\n\t\t\t\t\t\t\theight='200'\n\t\t\t\t\t\t\tmax = {biglog(data.max)}\n\t\t\t\t\t\t\tfill = 'white'\n\t\t\t\t\t\t/>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div style={barStyle}>\n\t\t\t\t\t\t<Component.Bars\n\t\t\t\t\t\t\tdata={data.colaz.map(v => biglog(v))}\n\t\t\t\t\t\t\twidth='800'\n\t\t\t\t\t\t\theight='200'\n\t\t\t\t\t\t\tmax = {biglog(data.max)}\n\t\t\t\t\t\t\tfill = 'red'\n\t\t\t\t\t\t/>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t</div>\n\t\t</div>\n}\n\nexport {Content}\n","/home/samuel/repo/showcase/collatz/frontend/src/view/frame/index.js",["98","99"],[],"\nconst Header = props => {\n\tlet  {logo, state, style} = props\n\n\treturn <div style={style}>\n\t\t\t<div style={style.logo}>\n\t\t\t\t<img style={style.logo.image} src={logo} />\n\t\t\t</div>\n\n\t\t\t<div><a style={{color: 'white'}} href='https://github.com/mone4da/showcase'>Collatz at github</a></div>\n\t\t\t<div>Author: Samuel Ferrer Col√°s from Havana, Cuba (born 1964)</div>\n\t\t</div>\n}\n\nconst Footer = props => {\n\tlet  {style, state} = props\n\n\treturn <div style={style}>\n\t\t\t{state.system.copyright}\n\t\t</div>\n}\n\nexport {Header, Footer}\n","/home/samuel/repo/showcase/collatz/frontend/src/view/component/inspector.js",[],[],"/home/samuel/repo/showcase/collatz/frontend/src/view/component/tab.js",["100","101","102","103","104","105","106","107"],[],"\nimport {useState} from 'react'\n\nconst Tab = props => {\n\tlet {id, data, selected} = props\n\n\tconst style = selected => {\n\t\treturn selected && {\n\t\t\t\t\tbackground: '#0404cc',\n\t\t\t\t\tcursor: 'default'\n\t\t\t\t}\n\t\t\t\t|| {\n\t\t\t\t\tbackground: '#08093b',\n\t\t\t\t\tcursor: 'pointer'\n\t\t\t\t}\n\t}\n\n\n\tlet handleClick = () => {\n\t\tprops.onClick && props.onClick()\n\t}\n\n\treturn\t<div key={id} style={{...style(selected),  display: 'flex', alignItems: 'center', justifyContent: 'center'}} onClick={handleClick}>\n\t\t\t{data}\n\t\t</div>\n}\n\nconst IconTab = props => {\n\tlet {id, data, selected} = props\n\n\tconst style = selected => {\n\t\treturn selected && {\n\t\t\t\t\tbackground: '#0404cc',\n\t\t\t\t\tcursor: 'default'\n\t\t\t\t}\n\t\t\t\t|| {\n\t\t\t\t\tbackground: '#08093b',\n\t\t\t\t\tcursor: 'pointer'\n\t\t\t\t}\n\t}\n\n\n\tlet handleClick = () => {\n\t\tprops.onClick && props.onClick()\n\t}\n\n\treturn\t<div key={id} style={{...style(selected), height: '30px', width: '30px', display: 'flex', alignItems: 'center', justifyContent: 'center'}} onClick={handleClick}>\n\t\t\t{data}\n\t\t</div>\n}\n\n\nconst Tabs = props => {\n\tlet {style, selection, data, onSelection} = props\n\n\tlet handleClick = id => {\n\t\tonSelection && onSelection(id)\n\t}\n\n\treturn \t<div style={style}>\n\t\t\t{data.map(item =>\n\t\t\t\titem.icon && <IconTab\n\t\t\t\t\tid = {item.id}\n\t\t\t\t\tdata = {item.icon}\n\t\t\t\t\tselected={item.id === selection}\n\t\t\t\t\tonClick = {() => handleClick(item.id)}\n\t\t\t\t\t/>\n\t\t\t\t\t||\n\t\t\t\t\t<Tab\n\t\t\t\t\tid = {item.id}\n\t\t\t\t\tdata = {item.data}\n\t\t\t\t\tselected={item.id === selection}\n\t\t\t\t\tonClick = {() => handleClick(item.id)}\n\t\t\t\t\t/>\n\t\t\t)}\n\t\t</div>\n}\n\nlet verticalStyle = tabSize => ({\n\tdisplay: 'grid',\n\tgridTemplateColumns: `${tabSize} auto`,\n\theight: '100%',\n\tpaddingRight: '2px',\n\n\ttabs: {\n\t\tdisplay: 'flex',\n\t\tflexDirection: 'column',\n\t\theight: '100%'\n\t}\n})\n\nlet horizontalStyle = tabSize => ({\n\tdisplay: 'grid',\n\tgridTemplateRows : `${tabSize} auto`,\n\theight: '100%',\n\twidth: '100%',\n\tpaddingRight: '2px',\n\n\ttabs: {\n\t\tdisplay: 'flex',\n\t\tflexDirection: 'row',\n\t\tjustifyContent: 'space-between',\n\t\theight: '100%'\n\t}